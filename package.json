{
  "name": "badagig",
  "version": "1.0.0",
  "engines": {
    "node": "8.7.0",
    "npm": "5.4.2"
  },
  "description": "badagig api includes chat funtionality.",
  "main": "dist",
  "scripts": {
    "dev": "NODE_ENV=development nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "prestart": "npm run -s build",
    "start": "node ./dist/index.js",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unsed-vars": 1
    }
  },
  "author": "Perfect Aduh",
  "license": "ISC",
  "dependencies": {
    "async": "^2.5.0",
    "aws-sdk": "^2.127.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.17.1",
    "dotenv": "5.0.0",
    "express": "^4.15.2",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^7.4.0",
    "mongodb": "^2.2.33",
    "mongoose": "^4.9.7",
    "mongoose-timestamp": "^0.6.0",
    "nodemailer": "^4.1.3",
    "passport": "^0.3.2",
    "passport-facebook-token": "^3.3.0",
    "passport-google-oauth20": "",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "socket.io": "^2.0.3",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "nodemon": "^1.11.0"
  }
}
